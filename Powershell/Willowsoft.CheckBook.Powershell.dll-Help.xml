    <command:examples>
      <command:example>
        <maml:title>Example 1: Retrieve subscription data</maml:title>
        <dev:code>PS C:\&gt;Get-UsageAggregates -ReportedStartTime "5/2/2015" -ReportedEndTime "5/5/2015"</dev:code>
        <dev:remarks>
          <maml:para>
