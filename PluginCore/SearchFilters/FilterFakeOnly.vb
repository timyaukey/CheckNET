Option Strict On
Option Explicit On

Public Class FilterFakeOnly
    Implements ISearchFilter

    Public Function IsIncluded(objTrx As BaseTrx) As Boolean Implements ISearchFilter.IsIncluded
        Return objTrx.IsFake And Not objTrx.IsAutoGenerated
    End Function

    Public Overrides Function ToString() As String
        Return "Fake (but not Generated)"
    End Function
End Class
